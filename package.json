{
  "name": "lavenza",
  "version": "1.0.0",
  "description": "Electron app to link between text hook and Quick Translator",
  "main": "index.js",
  "repository": "git@github.com:imouto1994/Lavenza.git",
  "author": "Trong Nhan <imouto1994@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev-renderer":
      "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/renderer.dev.js",
    "build-renderer":
      "cross-env NODE_ENV=production webpack --config ./webpack/renderer.prod.js",
    "dev-main":
      "cross-env NODE_ENV=development electron ./src/electron/index.js",
    "build-main":
      "cross-env NODE_ENV=production webpack --config ./webpack/electron.prod.js & electron ./build/electron.js",
    "build": "",
    "lint": "eslint src webpack index.js --fix",
    "format": "prettier --write '{src,webpack}/**/*.js'",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "flow": "flow",
    "flow-install": "flow-typed install"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "cache-loader": "1.0.3",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "devtron": "1.4.0",
    "electron-builder": "19.33.0",
    "electron-debug": "1.4.0",
    "electron-devtools-installer": "2.2.0",
    "electron-rebuild": "1.6.0",
    "eslint": "4.8.0",
    "eslint-config-fbjs-opensource": "1.0.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-flowtype": "2.37.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "flow-bin": "0.56.0",
    "flow-typed": "2.1.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "lint-staged": "4.2.3",
    "postcss-calc": "6.0.0",
    "postcss-discard-comments": "2.0.4",
    "postcss-loader": "2.0.6",
    "postcss-mixins": "6.1.1",
    "postcss-nested": "2.1.2",
    "postcss-simple-vars": "4.1.0",
    "prettier": "1.7.4",
    "redux-logger": "3.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  },
  "dependencies": {
    "antd": "2.13.4",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "electron": "1.7.8",
    "keymirror": "0.1.1",
    "lodash": "4.17.4",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-batched-actions": "0.2.0",
    "redux-batched-subscribe": "0.1.6",
    "redux-saga": "0.15.6",
    "reselect": "3.0.1",
    "source-map-support": "0.4.18"
  }
}
